{
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "env": {
      // "es6": true,
      "es2022": true,
      "jest": true,
      "browser": true,
      "node": true
    },
    "globals": {
      "$": true,
      "require": true,
      "process": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2022
    },
    "rules": {
      "no-unused-vars": "warn",
      "prettier/prettier": [
        "error",
        { "singleQuote": true,
          "endOfLine": "auto" 
        }
       
      ]     
    }
  }